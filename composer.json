{
    "name": "edwrodrig/js",
    "description": "Javascript front-end utils",
    "license": "MIT",
    "authors": [
        {
            "name": "Edwin Rodríguez León",
            "email": "edwrodrig@gmail.com",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "minimum-stability" : "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload" : {
      "psr-4" : {
        "edwrodrig\\js\\" : "src/"
      },
      "exclude-from-classmap" : ["src/js/"]
    },
    "autoload-dev" : {
        "psr-4" : {
            "test\\edwrodrig\\js\\" : "tests/"
        },
        "exclude-from-classmap" : ["tests/js/"]
    },
    "require-dev" : {
        "bower-asset/qunit": "^2.6",
        "phpunit/phpunit" : "^8.5"
    },
    "archive": {
        "exclude": [
            "/tests",
            ".gitignore",
            ".git",
            "/.travis.yml",
            "/phpunit.xml.dist",
            "/examples",
            "/.idea",
            "/README.md"
        ]
    }
}
